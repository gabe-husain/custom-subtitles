{
    "manifest_version": 3,
    "name": "__MSG_extName__",
    "version": "1.0",
    "description": "__MSG_extDescription__",
    "default_locale": "en",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
        "default_title": "__MSG_extTitle__",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["styles.css"],
        "js": ["content.js"]
    }]
}